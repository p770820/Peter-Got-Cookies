{
    "manifest_version": 3,
    "name": "Peter Got Cookies",
    "version": "1.0",
    "permissions": ["cookies"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    },
    "host_permissions": [
      "<all_urls>"
    ]
  }
  